#User function Template for python3

class Solution:
    
    #Function to return a list containing the union of the two arrays.
    def findUnion(self,a,b,n,m):
        '''
        :param a: given sorted array a
        :param n: size of sorted array a
        :param b: given sorted array b
        :param m: size of sorted array b
        :return:  The union of both arrays as a list
        '''
        # code here 
        j=0
        lst=[]
        i=0
        while(i<n and j<m):
            if a[i] in lst:
                i+=1
            elif b[j] in lst:
                j+=1
            elif a[i]==b[j]:
                lst.append(a[i])
                i+=1
                j+=1
            elif a[i]<b[j] :
                lst.append(a[i])
                i+=1
            elif a[i]>b[j]:
                lst.append(b[j])
                j+=1
        if i<n:
            for z in range(i,n):
                if a[z] not in lst:
                    lst.append(a[z])
            
        if j<m:
            for i in range (j,m):
                if b[i] not in lst:
                    lst.append(b[i])
        return lst
                

